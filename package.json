{
   "name": "monitor-txt",
   "version": "1.1.1",
   "description": "console utility for live monitoring",
   "type": "module",
   "exports": {
      "import": {
         "types": "./lib/src/index.d.ts",
         "default": "./lib/src/index.js"
      },
      "require": {
         "types": "./lib/src/index.d.cts",
         "default": "./lib/src/index.cjs"
      }
   },
   "bin": {
      "monitext": "./lib/cli/index.js"
   },
   "keywords": [
      "console",
      "logger",
      "structured-logging",
      "monitoring"
   ],
   "scripts": {
      "build": "vitest --run && tsup src/index.ts --format esm,cjs --dts --outDir ./lib/src && tsup src-cli/index.ts --format esm --outDir ./lib/cli"
   },
   "author": "",
   "license": "MIT",
   "dependencies": {
      "crypto-js": "^4.2.0",
      "inquirer": "^12.6.0"
   },
   "repository": {
      "type": "git",
      "url": "https://github.com/MisterChatlinks/monitor-txt.git"
   },
   "devDependencies": {
      "@types/crypto-js": "^4.2.2",
      "@types/node": "^22.15.2",
      "ts-node": "^10.9.2",
      "tsup": "^8.1.0",
      "vitest": "^3.1.2"
   }
}
